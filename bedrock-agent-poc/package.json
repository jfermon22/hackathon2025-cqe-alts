{
  "name": "bedrock-agent-poc",
  "version": "1.0.0",
  "description": "POC for interacting with AWS Bedrock Agent",
  "type": "module",
  "main": "bedrock-agent-test.js",
  "scripts": {
    "test": "node multi-function-agent-test.js",
    "test-agent": "node bedrock-agent-test.js",
    "test-hello": "node bedrock-agent-test.js",
    "test-search": "node amazon-search-agent-test.js",
    "test-search-json": "node amazon-search-agent-test.js --format=json",
    "test-search-nlp": "node amazon-search-agent-test.js --format=nlp",
    "test-multi": "node multi-function-agent-test.js",
    "test-multi-search": "node multi-function-agent-test.js --test=search",
    "test-multi-supplier": "node multi-function-agent-test.js --test=supplier",
    "test-browser": "node browser-compatible-agent-test.js",
    "test-browser-search": "node browser-compatible-agent-test.js --test=search",
    "test-browser-supplier": "node browser-compatible-agent-test.js --test=supplier",
    "test-all": "npm run test-hello && echo '\n' && npm run test-search",
    "start": "node multi-function-agent-test.js"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-agent-runtime": "^3.0.0"
  },
  "keywords": [
    "aws",
    "bedrock",
    "agent",
    "poc"
  ],
  "author": "Amazon",
  "license": "MIT"
}
